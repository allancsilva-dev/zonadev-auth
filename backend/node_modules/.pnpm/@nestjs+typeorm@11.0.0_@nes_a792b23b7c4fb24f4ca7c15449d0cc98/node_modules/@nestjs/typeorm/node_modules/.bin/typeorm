#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/proc/cygdrive/d/Projetos/zonadev-auth/backend/node_modules/.pnpm/typeorm@0.3.28_pg@8.19.0_ts_f26728d46bbe1fabc5bd13ff03a4bedd/node_modules/typeorm/node_modules:/proc/cygdrive/d/Projetos/zonadev-auth/backend/node_modules/.pnpm/typeorm@0.3.28_pg@8.19.0_ts_f26728d46bbe1fabc5bd13ff03a4bedd/node_modules:/proc/cygdrive/d/Projetos/zonadev-auth/backend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/proc/cygdrive/d/Projetos/zonadev-auth/backend/node_modules/.pnpm/typeorm@0.3.28_pg@8.19.0_ts_f26728d46bbe1fabc5bd13ff03a4bedd/node_modules/typeorm/node_modules:/proc/cygdrive/d/Projetos/zonadev-auth/backend/node_modules/.pnpm/typeorm@0.3.28_pg@8.19.0_ts_f26728d46bbe1fabc5bd13ff03a4bedd/node_modules:/proc/cygdrive/d/Projetos/zonadev-auth/backend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../typeorm@0.3.28_pg@8.19.0_ts_f26728d46bbe1fabc5bd13ff03a4bedd/node_modules/typeorm/cli.js" "$@"
else
  exec node  "$basedir/../../../../../../typeorm@0.3.28_pg@8.19.0_ts_f26728d46bbe1fabc5bd13ff03a4bedd/node_modules/typeorm/cli.js" "$@"
fi
